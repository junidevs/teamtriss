{
  "name": "teamtriss",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "SENTRY_LOG_LEVEL=debug next build --debug",
    "build:prod": "npm run build -- --no-lint",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "node --no-compilation-cache ./node_modules/next/dist/bin/next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "node --no-compilation-cache --expose-gc ./node_modules/jest/bin/jest.js --config=jest.config.ts --logHeapUsage",
    "test:coverage": "npm run test -- --coverage",
    "test:detect-leaks": "npm run test -- --detect-leaks",
    "test:watch": "yarn run test -- --watch",
    "test:ci": "npm run test -- --runInBand",
    "test:ci:detect-leaks": "npm run test:ci -- --detect-leaks",
    "test:typecheck": "tsc --project tsconfig.testsTypecheck.json",
    "prettier": "prettier . --write"
  },
  "dependencies": {
    "@testing-library/react": "^14.0.0",
    "@types/node": "18.16.3",
    "@types/react": "18.2.1",
    "@types/react-dom": "18.2.2",
    "eslint": "8.39.0",
    "eslint-config-next": "13.3.4",
    "jest": "^29.5.0",
    "next": "13.3.4",
    "next-translate": "^2.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.4.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-security": "1.4.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sonarjs": "0.12.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8"
  }
}
